function add( x,y) {
	return x+y
}
console.log(add(123,321) ) 
//Topic: arrow function
const addition = (x,y) => { 
	return x+y
}
console.log(addition(1,2))

const subtraction = (x,y) => x-y
console.log(subtraction(45,54) )
//Datatypes
const user = () =>{
	let name1 = "Mohammed" //string
	let age1 = 21 //number
	let isMarried1 = true //boolean
	let hobbies1 = ["typing","sleeping","netflix"] //object
	let address1 ={ 
		"Line1":"Hno:1234567/b/b123 ",
		"Line2":"",
		"city" : "NewYork City",
		"state":"NY"
		"zipcode":"1234432343",


	
	} 
	let user1 = {
		name:name1, 
		age:age1,
		isMarried:isMarried1,
		hobbies:hobbies1,
		address:address1,

	}
	return user1
	
}
console.log(user())
